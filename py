a = float(input("Digite o valor de A: "))
print(a)
b =float(input("Digite o valor de B:"))
print(b)
x =2.0*a+3.0*b
print(x)


horasTrabalhadas=int(input("Digite suas horas trabalhadas:"))
valorHorasTrabalhadas=int(input("Digite o valor que recebe por hora: "))
valordosDescontos=int(input("Digite o valor dos descontos mensais: "))
valor_final =horasTrabalhadas*valorHorasTrabalhadas-valordosDescontos
print(valor_final)
